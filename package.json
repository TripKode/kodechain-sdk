{
  "name": "@kodechain/sdk-ts",
  "version": "1.0.2",
  "description": "Official JavaScript/TypeScript SDK for KodeChain blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "kodechain",
    "blockchain",
    "web3",
    "smart-contracts",
    "dpos",
    "pbft",
    "typescript",
    "sdk"
  ],
  "author": "KodeChain Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kodechain/web-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@noble/hashes": "^2.0.1",
    "@noble/post-quantum": "^0.5.4",
    "axios": "^1.6.0",
    "bignumber.js": "^9.1.0",
    "eventemitter3": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.19.33",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nock": "^14.0.11",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}